start
    : typeDef start
    | EPSILON
    ;
typeDef
    : IDENTIFIER varList ';'
    ;
varList
    : varDef varListCont
    ;
varListCont
    : ',' varList
    | EPSILON
    ;
varDef
    : IDENTIFIER
    | '*' varDef
    ;

dummy
    : ',' SEMICOLON
    | '*'
    ;

WHITESPACE : HIDDEN /[ \n\t]/ ;
IDENTIFIER : /[a-zA-Z][a-zA-Z0-9_]*/ ;
